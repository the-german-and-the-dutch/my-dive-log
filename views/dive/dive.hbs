{{#each this.dive}}
<h1>Hello {{ username }}</h1>




<h2>Dive Details</h2>

<h3>Dive on {{ date }}</h3>
<ul>
    <li>Location {{ location }} </li>
    <li>Dive number {{ diveNumber }}</li>
    <li>Time in {{ timeIn }} </li>
    <li>Time out {{ timeOut }} </li>
    <li> Bottom time {{ bottomTime }} </li>
    <li> Depth {{ depth }} </li>
    <li>Air start {{ airStart }} </li>
    <li>Air end {{ airEnd }} </li>
    <li>Visibility {{ visibility }} </li>
    <p> {{ comment }}</p>
</ul>



<a href="/"> go back</a>



{{/each}}